{
    "name": "PulseHax",
    "version": "0.10.7",
    "description": "Hacking pulsus has never been so easy",
    "permissions": ["storage"],
    "host_permissions": ["https://pulsus.cc/*","https://www.pulsus.cc/*"],
    "content_scripts": [
      {
        "matches": ["https://pulsus.cc/*","https://www.pulsus.cc/*"],
        "js": ["contentScript.js"],
        "all_frames": true
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "Init.js",
          "inGamePort.js",
          "assets/retry.wav",
          "assets/scroll.wav",
          "assets/load.wav",
          "assets/icon.png",
          "assets/icon.ico",
          "*"
        ],
        "matches": ["https://pulsus.cc/*","https://www.pulsus.cc/*"]
      }
    ],
    "action": {
      "default_icon": {
        "32": "assets/PulseHax.png"
      },
      "default_title": "PulseHax",
      "default_popup": "popup.html"
    },
    "manifest_version": 3
}
